<?xml version="1.0"?>
<!DOCTYPE MODE SYSTEM "xmode.dtd">
<MODE>
	<PROPS>
		<PROPERTY NAME="lineComment" VALUE="#" />
	</PROPS>
	<RULES HIGHLIGHT_DIGITS="FALSE" IGNORE_CASE="FALSE">
		
		<EOL_SPAN TYPE="COMMENT1" AT_LINE_START="TRUE">#</EOL_SPAN>
		
		<!-- these seem fine -->
		<MARK_PREVIOUS TYPE="METHOD" EXCLUDE_MATCH="FALSE">development:</MARK_PREVIOUS>
		<MARK_PREVIOUS TYPE="METHOD" EXCLUDE_MATCH="FALSE">test:</MARK_PREVIOUS>
		<MARK_PREVIOUS TYPE="METHOD" EXCLUDE_MATCH="FALSE">production:</MARK_PREVIOUS>
		<MARK_PREVIOUS TYPE="METHOD" EXCLUDE_MATCH="FALSE">first:</MARK_PREVIOUS>
		<MARK_PREVIOUS TYPE="METHOD" EXCLUDE_MATCH="FALSE">another:</MARK_PREVIOUS>
		
		<KEYWORDS>
			<KEYWORD1>development</KEYWORD1>
			<KEYWORD1>test</KEYWORD1>
			<KEYWORD1>production</KEYWORD1>
			<KEYWORD1>first</KEYWORD1>
			<KEYWORD1>another</KEYWORD1>
			
			<LITERAL2>adapter</LITERAL2>
			<LITERAL2>database</LITERAL2>
			<LITERAL2>username</LITERAL2>
			<LITERAL2>password</LITERAL2>
			<LITERAL2>socket</LITERAL2>
			<LITERAL2>host</LITERAL2>
			<LITERAL2>port</LITERAL2>
			<LITERAL2>schema_search_path</LITERAL2>
			<LITERAL2>encoding</LITERAL2>
			<LITERAL2>min_messages</LITERAL2>
		</KEYWORDS>
	</RULES>
</MODE>