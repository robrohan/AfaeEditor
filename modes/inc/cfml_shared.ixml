<!-- 
	cfml_shared.ixml: 
	Shared rules for CFML syntax highlighting (things that are common
	to both normal CFML and CFSCRIPT)
-->
<SEQ TYPE="NULL">)</SEQ>
<SEQ TYPE="NULL">(</SEQ>
<SEQ TYPE="NULL">[</SEQ>
<SEQ TYPE="NULL">]</SEQ>
<SEQ TYPE="NULL">,</SEQ>

<SEQ TYPE="OPERATOR">/</SEQ>
<SEQ TYPE="OPERATOR">+</SEQ>
<SEQ TYPE="OPERATOR">-</SEQ>
<SEQ TYPE="OPERATOR">*</SEQ>
<SEQ TYPE="OPERATOR">&amp;</SEQ>
<SEQ TYPE="OPERATOR">=</SEQ>

<!-- Railo's ternary operator: -->
<SEQ TYPE="OPERATOR">?</SEQ>
<SEQ TYPE="OPERATOR">:</SEQ>

